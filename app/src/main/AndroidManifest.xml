<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sacol.homecontents">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hc_main_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/hc_main_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.StatusBar">
        <activity android:name="kr.hs.emirim.homecontents.Personalinfo"></activity>
        <activity android:name="kr.hs.emirim.homecontents.PersonalInfo" />
        <activity
            android:name="kr.hs.emirim.homecontents.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="kr.hs.emirim.homecontents.DetailEditActivity" />
        <activity android:name="kr.hs.emirim.homecontents.MypageEditActivity" />
        <activity android:name="kr.hs.emirim.homecontents.DetailActivity" />
        <activity android:name="kr.hs.emirim.homecontents.SettingActivity" />
        <activity android:name="kr.hs.emirim.homecontents.MainActivity" />
        <activity android:name="kr.hs.emirim.homecontents.LoginActivity" />
        <activity android:name="kr.hs.emirim.homecontents.MypageActivity" />
        <activity android:name="kr.hs.emirim.homecontents.PlusActivity" />
        <activity android:name="kr.hs.emirim.homecontents.SignupActivity" />
        <activity android:name="kr.hs.emirim.homecontents.SearchActivity" />
        <activity android:name="kr.hs.emirim.homecontents.MystorageActivity" />
    </application>

</manifest>